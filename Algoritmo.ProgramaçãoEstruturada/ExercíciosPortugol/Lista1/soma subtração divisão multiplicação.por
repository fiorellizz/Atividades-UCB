programa {
  funcao inicio() {
    real = num1, num2, soma, sub, div, mult
    escreva("Digite 2 números: \n")
    leia(num1, num2)
    soma = num1 + num2
    sub = num1 - num2
    div = num1 / num2
    mult = num1 * num2
    escreva("O resultado da soma é ",soma, "\nO resultado da subração é ",sub, "\nO resultado da divisão é: ",div, "\nO resultado da multiplicação é ",mult,)
  }
}
